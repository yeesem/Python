        "def ticket(speed,isBirthday):\n",
        "  if(isBirthday!=True):\n",
        "   if speed <= 60:\n",
        "     print(\"No Ticket\")\n",
        "   elif speed >60 and speed <=80:\n",
        "     print(\"Small Ticket\")\n",
        "   else:\n",
        "     print(\"No Ticket\")\n",
        "  else:\n",
        "    if speed <= 65:\n",
        "     print(\"No Ticket\")\n",
        "    elif speed >65 and speed <=85:\n",
        "     print(\"Small Ticket\")\n",
        "    else:\n",
        "     print(\"No Ticket\")"
      ],
      "metadata": {
        "id": "eEYld2Idtr2y"
      },
      "execution_count": 19,
      "outputs": []
    },
